{
  "name": "cli-frontend",
  "version": "1.0.0-prebeta.0",
  "description": "command line interface for create defaultly the project with the tools needs and requires for initialzer in React or nextjs with ts and cleanners like prettier, eslint and lint-staged",
  "bin": {
    "cli-frontend": "./bin/run",
    "rn": "./bin/rn"
  },
  "author": {
    "email": "juanestban000@gmail.com",
    "name": "Juan Esteban Echeverry Perdomo",
    "url": "https://github.com/Juanestban"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Juanestban/cli-frontend"
  },
  "private": true,
  "license": "ISC",
  "scripts": {
    "init": "tsc --init",
    "start:run": "node ./bin/run",
    "generate": "node ./bin/run",
    "dev": "nodemon",
    "build": "tsc",
    "compile:exec:run": "yarn build && yarn start:run",
    "compile:generate": "yarn build && yarn generate",
    "postinstall": "yarn husky install"
  },
  "keywords": [
    "inquirer",
    "react",
    "next",
    "typescript",
    "prettier",
    "eslint",
    "lint-staged",
    "sveltejs",
    "vuejs"
  ],
  "bugs": {
    "url": "https://github.com/Juanestban/cli-frontend/issues"
  },
  "homepage": "https://github.com/Juanestban/cli-frontend",
  "dependencies": {
    "inquirer": "8.1.0",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "7.3.1",
    "husky": "7.0.2",
    "nodemon": "2.0.13",
    "ts-node": "10.3.0",
    "typescript": "4.3.2"
  }
}
